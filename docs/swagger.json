            },
            "required": true
          },
          {
            "in": "query",
            "name": "month",
            "schema": {
              "type": "integer",
              "example": 12
            },
            "required": true
          },
          {
            "in": "query",
            "name": "year",
            "schema": {
              "type": "integer",
              "example": 2024
            },
            "required": true
          }
